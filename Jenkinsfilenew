def pipeline
    node('jenkins-master') {
        dir('mavenpipeline')
        {
        git credentialsId: '' , branch: 'master', url: 'https://github.com/kumartdp/mavenpipeline.git'
         }
        pipeline = load 'mavenpipeline/pipeline.groovy'
        pipeline.helloworld()
        pipeline.addition()
        pipeline.build()
        pipeline.deployimage()
        
        
    }
